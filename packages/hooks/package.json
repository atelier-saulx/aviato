{
  "name": "@aviato/hooks",
  "description": "A library of React hooks for Aviato",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/atelier-saulx/aviato-ui",
  "sideEffects": false,
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "iife": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@aviato/utils": "*",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@aviato/tsconfig": "*",
    "@aviato/eslint-config": "*",
    "@aviato/prettier-config": "*",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "eslint": "7.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "typescript": "4.5.2",
    "tsup": "^5.10.1"
  },
  "prettier": "@aviato/prettier-config",
  "eslintConfig": {
    "extends": "@aviato/eslint-config"
  },
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "lint": "TIMING=1 run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:prettier": "prettier --write './src/**/*.ts{,x}'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  }
}
