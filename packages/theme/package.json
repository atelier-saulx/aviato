{
  "name": "@aviato/theme",
  "version": "1.0.0",
  "description": "Theme for Aviato",
  "license": "MIT",
  "dependencies": {
    "@stitches/react": "^1.2.5"
  },
  "type": "module",
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^16.11.10",
    "chalk": "^5.0.0",
    "esm": "^3.2.25",
    "nodemon": "^2.0.15",
    "ora": "^6.0.1",
    "rollup": "^2.33.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "token-transformer": "^0.0.13",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "prettier": "@saulx/prettier-config",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "lint": "run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint --ignore-path .gitignore './**/*.{ts,tsx}'",
    "parse": "run-s parse:*",
    "parse:tokens": "nodemon ./scripts/parseToken.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js"
}
