{
  "name": "@aviato/ui",
  "version": "3.0.0",
  "description": "A library of front-end React components",
  "license": "MIT",
  "repository": "https://github.com/atelier-saulx/aviato-ui",
  "sideEffects": false,
  "dependencies": {
    "@aviato/utils": "*",
    "@stitches/react": "^1.2.6",
    "react": "^17.0.2",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@aviato/eslint-config": "*",
    "@aviato/prettier-config": "*",
    "@svgr/cli": "^5.1.0",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/throttle-debounce": "^2.1.0",
    "chalk": "^5.0.0",
    "eslint": "7.28.0",
    "esm": "^3.2.25",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "ora": "^6.0.1",
    "prettier": "2.5.1",
    "promisify-child-process": "^4.1.1",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup": "^2.60.2",
    "svgo": "^2.8.0",
    "typescript": "4.5.2"
  },
  "prettier": "@aviato/prettier-config",
  "eslintConfig": {
    "extends": "@aviato/eslint-config"
  },
  "scripts": {
    "nodemon:parse": "nodemon --exec yarn parse",
    "scripts": "run-s scripts:*",
    "scripts:icons": "node --experimental-modules ./scripts/reactFromSVG.mjs",
    "scripts:tokens": "node --experimental-modules ./scripts/parseToken.mjs",
    "start": "yarn watch",
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "lint": "run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:prettier": "prettier --write './src/**/*.ts{,x}'"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js"
}
