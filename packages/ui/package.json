{
  "name": "@based/ui-next",
  "version": "3.0.0",
  "repository": "https://github.com/atelier-saulx/based-ui",
  "description": "A library of front-end React components for Based",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@saulx/eslint-config": "^1.1.1",
    "@saulx/hash": "^1.1.0",
    "@saulx/prettier-config": "^1.0.0",
    "@types/node": "^16.9.4",
    "@types/react": "^17.0.22",
    "@types/styled-components": "^5.1.15",
    "tsc-watch": "^4.5.0",
    "typescript": "4.4.3"
  },
  "sideEffects": false,
  "prettier": "@saulx/prettier-config",
  "eslintConfig": {
    "extends": "@saulx/eslint-config",
    "rules": {
      "react/jsx-curly-newline": 0,
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "scripts": {
    "start": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./postbuild.js\" --onFailure \"echo Build Failed\""
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "license": "MIT"
}
