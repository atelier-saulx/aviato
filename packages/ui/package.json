{
  "name": "@aviato/ui",
  "description": "A library of Front-end React components",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/atelier-saulx/aviato-ui",
  "sideEffects": false,
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "iife": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@aviato/hooks": "*",
    "@aviato/utils": "*",
    "@stitches/react": "^1.2.6",
    "next-themes": "^0.0.15",
    "react": "^17.0.2",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@aviato/eslint-config": "*",
    "@aviato/prettier-config": "*",
    "@aviato/tsconfig": "*",
    "@svgr/cli": "^5.1.0",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/throttle-debounce": "^2.1.0",
    "chalk": "^5.0.0",
    "esbuild-plugin-copy": "^0.3.0",
    "eslint": "7.28.0",
    "esm": "^3.2.25",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "ora": "^6.0.1",
    "prettier": "2.5.1",
    "promisify-child-process": "^4.1.1",
    "react-textarea-autosize": "^8.3.3",
    "svgo": "^2.8.0",
    "tsup": "^5.10.1",
    "typescript": "4.5.2"
  },
  "prettier": "@aviato/prettier-config",
  "eslintConfig": {
    "extends": "@aviato/eslint-config"
  },
  "scripts": {
    "types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir ./dist",
    "nodemon": "nodemon --exec yarn parse",
    "parse": "run-s parse:*",
    "parse:tokens": "node --experimental-modules ./scripts/parseTokens.mjs",
    "parse:icons": "node --experimental-modules ./scripts/reactFromSVG.mjs",
    "watch": "tsup ./src/index.ts --watch --onSuccess 'yarn types'",
    "build": "tsup ./src/index.ts",
    "lint": "run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:prettier": "prettier --write './src/**/*.ts{,x}'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  }
}
