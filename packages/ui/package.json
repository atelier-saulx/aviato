{
  "name": "@aviato/ui",
  "version": "3.0.0",
  "description": "A library of front-end React components",
  "license": "MIT",
  "repository": "https://github.com/atelier-saulx/aviato-ui",
  "dependencies": {
    "@aviato/theme": "*",
    "@aviato/utils": "*",
    "@stitches/react": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@saulx/eslint-config": "^1.1.1",
    "@saulx/prettier-config": "^1.0.0",
    "@types/node": "^16.9.4",
    "@types/react": "^17.0.22",
    "@types/throttle-debounce": "^2.1.0",
    "eslint": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.0",
    "rollup": "^2.33.1",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "4.4.3"
  },
  "sideEffects": false,
  "prettier": "@saulx/prettier-config",
  "scripts": {
    "build": "run-p build:*",
    "build:ui": "rollup -c --config rollup.config.js",
    "watch": "run-p watch:*",
    "watch:ui": "rollup -c --watch",
    "lint": "run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:prettier": "prettier --write './src/**/*.ts{,x}'"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js"
}
