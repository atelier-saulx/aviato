{
  "name": "@aviato/ui",
  "version": "3.0.0",
  "description": "A library of front-end React components",
  "license": "MIT",
  "repository": "https://github.com/atelier-saulx/aviato-ui",
  "dependencies": {
    "@stitches/react": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@aviato/theme": "*"
  },
  "devDependencies": {
    "@saulx/eslint-config": "^1.1.1",
    "@saulx/hash": "^1.1.0",
    "@saulx/prettier-config": "^1.0.0",
    "@types/node": "^16.9.4",
    "@types/react": "^17.0.22",
    "rollup": "^2.33.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "4.4.3",
    "npm-run-all": "^4.1.5"
  },
  "sideEffects": false,
  "prettier": "@saulx/prettier-config",
  "eslintConfig": {
    "extends": "@saulx/eslint-config",
    "rules": {
      "react/jsx-curly-newline": 0,
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "lint": "run-p lint:*",
    "lint:ts": "tsc --project ./tsconfig.json --noEmit"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js"
}
