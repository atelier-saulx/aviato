{
  "name": "@aviato/ui",
  "description": "A library of Front-end React components",
  "version": "0.3.11",
  "license": "MIT",
  "repository": "https://github.com/atelier-saulx/aviato-ui",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "dependencies": {
    "@aviato/utils": "*",
    "@popperjs/core": "^2.11.2",
    "@radix-ui/react-scroll-area": "^0.1.3",
    "@radix-ui/react-toggle-group": "^0.1.4",
    "@stitches/react": "^1.2.6",
    "prism-react-renderer": "^1.2.1",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-live": "2.4.1",
    "react-popper": "^2.2.5",
    "react-textarea-autosize": "^8.3.3"
  },
  "devDependencies": {
    "@aviato/eslint-config": "*",
    "@aviato/prettier-config": "*",
    "@aviato/tsconfig": "*",
    "@svgr/cli": "^5.1.0",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "chalk": "^5.0.0",
    "eslint": "7.28.0",
    "esm": "^3.2.25",
    "esno": "^0.14.0",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "next": "12.0.7",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "ora": "^6.0.1",
    "prettier": "2.5.1",
    "promisify-child-process": "^4.1.1",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0",
    "typedoc": "^0.22.11",
    "typescript": "4.5.4"
  },
  "prettier": "@aviato/prettier-config",
  "eslintConfig": {
    "extends": "@aviato/eslint-config"
  },
  "scripts": {
    "types": "esno ../../scripts/exec types packages/ui",
    "watch": "esno ../../scripts/exec watch packages/ui",
    "build": "esno ../../scripts/exec build packages/ui",
    "nodemon": "nodemon --exec yarn parse",
    "parse": "run-s parse:*",
    "parse:tokens": "esno scripts/parseTokens",
    "parse:icons": "esno scripts/reactFromSVG",
    "parse:lint": "yarn lint",
    "start": "yarn watch",
    "docs": "rimraf docs && typedoc --out docs src/index.ts",
    "lint": "run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:prettier": "prettier --write './src/**/*.ts{,x}'"
  }
}
