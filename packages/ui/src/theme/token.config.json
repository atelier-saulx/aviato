{
  "color": {
    "color-token-primary": "98, 0, 238",
    "color-token-secondary": "217, 19, 174",
    "color-token-background": "247, 247, 248",

    "color-primary": "rgb(var(--color-token-primary))",
    "color-secondary": "rgb(var(--color-token-secondary))",
    "color-tertiary": "rgba(var(--color-token-primary), 0.5)",
    "color-background": "rgb(var(--color-token-background))"
  },

  "size": {
    "size-token-xxs": "4",
    "size-token-xs": "8",
    "size-token-sm": "12",
    "size-token-md": "16",
    "size-token-lg": "200",

    "size-xxs": "calc(var(--size-token-xxs) * 1px)",
    "size-xs": "calc(var(--size-token-xs) * 1px)",
    "size-sm": "calc(var(--size-token-sm) * 1px)",
    "size-md": "calc(var(--size-token-md) * 1px)",
    "size-lg": "calc(var(--size-token-lg) * 1px)"
  }
}