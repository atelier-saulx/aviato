{
  "color": {
    "token-color-primary": "98, 0, 238",
    "token-color-secondary": "217, 19, 174",
    "token-color-background": "247, 247, 248",

    "color-primary": "rgb(var(--token-color-primary))",
    "color-secondary": "rgb(var(--token-color-secondary))",
    "color-tertiary": "rgba(var(--token-color-primary), 0.5)",
    "color-background": "rgb(var(--token-color-background))"
  },

  "size": {
    "token-size-xxs": "4",
    "token-size-xs": "8",
    "token-size-sm": "12",
    "token-size-md": "16",
    "token-size-lg": "20",
    "token-size-xl": "24",
    "token-size-xxl": "28",

    "size-xxs": "calc(var(--token-size-xxs) * 1px)",
    "size-xs": "calc(var(--token-size-xs) * 1px)",
    "size-sm": "calc(var(--token-size-sm) * 1px)",
    "size-md": "calc(var(--token-size-md) * 1px)",
    "size-lg": "calc(var(--token-size-lg) * 1px)",
    "size-xl": "calc(var(--token-size-xl) * 1px)",
    "size-xxl": "calc(var(--token-size-xxl) * 1px)"
  }
}