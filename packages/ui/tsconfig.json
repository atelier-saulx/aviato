{
  "extends": "@aviato/tsconfig/src/react-library.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "./src",
    "paths": {
      "~/*": ["src/*"]
    },
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "src/__test__/**/*",
    "src/icons/svg/**/*",
    "src/icons/parsed/**/*",
  ],
}