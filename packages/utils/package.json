{
  "name": "@aviato/utils",
  "description": "A library of utility functions for Aviato",
  "version": "0.3.11",
  "license": "MIT",
  "repository": "https://github.com/atelier-saulx/aviato-ui",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "react": ">=17.0.2"
  },
  "devDependencies": {
    "@aviato/eslint-config": "*",
    "@aviato/prettier-config": "*",
    "@aviato/tsconfig": "*",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "eslint": "7.28.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "tsup": "5.11.13",
    "esno": "^0.14.0",
    "typedoc": "^0.22.11",
    "typescript": "4.5.4"
  },
  "prettier": "@aviato/prettier-config",
  "eslintConfig": {
    "extends": "@aviato/eslint-config"
  },
  "scripts": {
    "types": "esno ../../scripts/exec types packages/utils",
    "watch": "esno ../../scripts/exec watch packages/utils",
    "build": "esno ../../scripts/exec build packages/utils",
    "docs": "rimraf docs && typedoc --out docs src/index.ts",
    "lint": "TIMING=1 run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:prettier": "prettier --write './src/**/*.ts{,x}'"
  }
}
