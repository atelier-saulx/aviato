{
  "name": "@aviato/utils",
  "version": "0.1.0",
  "description": "A library of utility functions for Aviato",
  "license": "MIT",
  "repository": "https://github.com/atelier-saulx/aviato-ui",
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@saulx/eslint-config": "^1.1.1",
    "@saulx/prettier-config": "^1.0.0",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rollup": "^2.60.2",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "4.5.2"
  },
  "sideEffects": false,
  "prettier": "@saulx/prettier-config",
  "scripts": {
    "start": "yarn watch",
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "lint": "run-p lint:*",
    "lint:typescript": "tsc --project ./tsconfig.json --noEmit",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:prettier": "prettier --write './src/**/*.ts{,x}'"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js"
}
