{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        "cjs/**",
        "esm/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "watch": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "release": {
      "outputs": []
    }
  }
}
